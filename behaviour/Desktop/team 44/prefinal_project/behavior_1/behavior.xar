                            <script language="4">
                                <content>
                                    <![CDATA[from BaseHTTPServer import BaseHTTPRequestHandler, HTTPServer
import os
from urlparse import urlparse, parse_qs
import requests

current_object = None

class NaoRemoteHTTPRequestHandler(BaseHTTPRequestHandler):

  def do_GET(self):

    if not current_object:
        return

    #print('Got GET...')
    #print(self.path)

    #urlparse(self.path).query
    #q = urlparse(self.path).query
    #parsed_q = parse_qs(q)
    #current_object.logger.info("\n Path_etic: ", self.headers)
    #current_object.logger.info(self.path)
    #current_object.logger.info(self.headers)

    #msg = self.headers

    ##self.path -> msg['action']

    #current_object.logger.info (self.headers[0])
    #current_object.logger.info (self.headers)
    #current_object.logger.info ()

    #raw_request = "http://127.0.0.1:4169" + self.path
    ##raw_request = "127.0.0.1:4169/lel?action=red&text=fuck"

    #---------------
    urlparse(self.path).query
    q = urlparse(self.path).query
    parsed_q = parse_qs(q)
    current_object.logger.info (parsed_q)
    #----------------

    action = (parsed_q ['action']) [0]
    text = (parsed_q ['text']) [0]

    #current_object.logger.info (raw_request)

    #parsed = urlparse.urlparse (raw_request)
    #action = (urlparse.parse_qs (parsed.query ))['action']
    #text = (urlparse.parse_qs (parsed.query ))['text']

    current_object.logger.info (action)
    #current_object.logger.info (text)

    response = 200

    #set global
    if action != "/free":
        ALMemory.insertData("nao-remote/is_action_running", 1)

    if action == "/red":
        current_object.onRedEyes()

    elif action == "/blue":
        current_object.onBlueEyes()

    elif action == "/green":
        current_object.onGreenEyes()

    elif action == "/dance":
        current_object.onDance()

    elif action == "/stand":
        current_object.onStand()

    elif action == "/sit":
        current_object.onSit()

    elif action == "/rest":
        current_object.onRest()

    elif action == "/wake":
        current_object.onWake()

    elif action == "/speak":
        #current_object.onHi(text)
        current_object.onHi(text)

    elif action == "/hi":
        current_object.onHi("Привет!")

    elif action == "/bye":
        current_object.onHi("Пока!")

    elif action == "/name":
        current_object.onHi("Меня зовут Нао.")

    elif action == "/color":
        current_object.onHi("Какого, цвета, мои, глазааа?")

    elif action == "/touch_left_hand":
        current_object.onHi("Потрогай мою левую руку.")

    elif action == "/touch_right_hand":
        current_object.onHi("Потрогай мою правую руку.")

    elif action == "/touch_left_foot":
        current_object.onHi("Потрогай мою левую ногу.")

    elif action == "/touch_right_foot":
        current_object.onHi("Потрогай мою правую ногу.")

    elif action == "/touch_head":
        current_object.onHi("Потрогай мою голову.")

    elif action == "/good":
        current_object.onHi("Молодец!")

    elif action == "/open_hands":
        current_object.openHands()

    elif action == "/close_hands":
        current_object.closeHands()

    elif action == "/breathe":
        current_object.Breathe()

    elif action == "/hello":
        current_object.Hello()

    elif action == "/wipe_forehead":
        current_object.Wipe ()


    elif action == "/free":
        #ALMemory.insertData("nao-remote/is_action_running", 0)
        status = ALMemory.getData("nao-remote/is_action_running")
        #self.send_response (response + 666)
        response = status + 666

    else:
        ALMemory.insertData("nao-remote/is_action_running", 0)
        #self.send_response (404)
        response = 404

    #send code 200 response
    self.send_response (response)

    self.send_header('Content-type','text-html')
    self.end_headers()

        #send file content to client
    self.wfile.write(self.path)

class MyClass(GeneratedClass):
    def __init__(self):
        global current_object
        GeneratedClass.__init__(self)
        current_object = self

        #ALMemory.insertData("nao-remote/is_action_running", 1)

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onRedEyes() #activate the output of the box
        self.logger.info('http server is starting...')
#        self.logger.info( current_object )
        #ip and port of servr
        #by default http server port is 80

        #the port stays busy after the execution!!!111!!!!!
        server_address = ('192.168.1.249', 8179)
        httpd = HTTPServer(server_address, NaoRemoteHTTPRequestHandler)
        httpd.serve_forever()
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
#        self.onRedEyes() #activate the output of the box]]>
</content>
